{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.templates.enabled": true,
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.defaultFileType": "mdx",
  "frontMatter.content.pageFolders": [
    {
      "title": "Pages",
      "path": "[[workspace]]/src/data/pages",
      "excludeSubdir": true
    },
    {
      "title": "Blog posts",
      "path": "[[workspace]]/src/data/docs"
    }
  ],
  "frontMatter.content.publicFolder": "public",
  "frontMatter.dashboard.openOnStart": false,
  "frontMatter.framework.id": "next",
  "frontMatter.framework.startCommand": "yarn dev",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.preview.pathName": "blog",
  "frontMatter.templates.prefix": "",
  "frontMatter.taxonomy.tags": [
    "CSS Modules",
    "Codespaces",
    "ColofulBox",
    "Docker",
    "FrontMatter",
    "GitHub",
    "Laravel",
    "Lit",
    "Nodejs",
    "Stylelint",
    "Web Components",
    "Webpack",
    "Workflow",
    "note"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Origin",
          "name": "origin",
          "type": "choice",
          "choices": [
            "qiita",
            "zenn",
            "hatena"
          ]
        }
      ]
    }
  ]
}